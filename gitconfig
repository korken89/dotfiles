[user]
    email = emil.fresk@gmail.com
    name = Emil Fresk
[pull]
    rebase = true
[alias]
    tree = log --oneline --decorate --all --graph
    co = checkout
    br = branch
    st = status
    unstage = reset HEAD --
    last = log -1 HEAD
    visual = !gitk
    ci = commit
    cloner = clone --recursive
    history = log -p --
    hist = log --pretty=format:'%C(yellow)[%ad]%C(reset) %C(green)[%h]%C(reset) | %C(red)%s %C(bold red){{%an}}%C(reset) %C(blue)%d%C(reset)' --graph --date=short
[push]
    default = simple
[core]
    editor = hx
    pager = delta
[merge]
	ff = false
[rebase]
    autoStash = true
[branch "master"]
    push = remote
[color]
	ui = auto
[diff]
	external = difft
